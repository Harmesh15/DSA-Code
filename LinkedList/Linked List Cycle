var hasCycle = function(head) {

   
    let fast = head;
    let slow = head;
 
    if(fast==null){
        return false;
    }

    while(fast !==null){
         
         if(fast.next == null){
            break;
         }

        fast = fast.next.next;
        slow = slow.next;

         if(fast === slow){
            return true;
         }
    }
    return false;

};
