var insertAtMiddle = function (head, k) {

    let newNode = new ListNode(k);
    let fast = head;
    let slow = head;

    while (fast.next !== null && fast.next.next !== null) {
        fast = fast.next.next;
        slow = slow.next;
    }

    newNode.next = slow.next;
    slow.next = newNode;

    return head;
