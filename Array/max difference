#include<iostream>
using namespace std;
    
    int MaxDiff(int arr[],int n){
    	int temp = 0; int res=0;
    	res	= arr[1] - arr[0];
    	for(int i=0; i<n; i++){
    		for(int j=i+1; j<n; j++){
    
    			res = max(res,arr[j] - arr[i]); 
				}
			} return res;
		}
	
	int main(){
		
		int arr[] = {2,3,5,6,4,8,5};
		
		cout<<MaxDiff(arr,6);
		
	}
