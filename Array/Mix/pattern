
function print_pattern(){
     /* Function to print the pattern */

    for (let i = 1; i <= 5; i++){

        var str = "";

        for (let j = 10; j <= 15-i; j++){
            str = str + "*";
        }
               
        for (let k = 10; k <= 7 + 2 * i; k++){
            str = str + " ";
        }

        for (let l = 10; l <= 15 - 1 * i; l++){
            str = str + "*";
        }
        console.log(str);
    }
    
   
}
