

function countPrime(n){
    
    const arr = Array(n).fill(true);
    let count = 0;
    arr[0] = false;
    arr[1] = false;
    
    
    for(let i=2; i<n; i++){
        
        if(arr[i]==true){
            count++;
            
        for(let j = i*i; j<n; j = j+i){
            arr[j] = false;
        }    
        }
    }
    return count;
  
}


