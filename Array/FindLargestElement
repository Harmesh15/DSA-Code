#include<iostream>
using namespace std;

  int Findlargest(double arr[] ,int n){
  	int max = 0;
  	for(int i=0; i<n; i++){
  		if(arr[i]>max){
  			max = i;
		  }
	  }
	  return max;
  }
      
   int main(){
   	int array[] = {4, 2, 10, 13, 2};
   	double array[] = {2.4, 5.2, 3.1, 1.5};
   	int n =5;
   	cout<<Findlargest(array,n);
   }
