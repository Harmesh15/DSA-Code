


function rotate(nums,k){
let n = nums.length;  
  k = k % n;
    
    reverse(nums,0,n-1)
    reverse(nums,0,k-1);
    reverse(nums,k,n-1);
    return nums;
}
function reverse(nums,l,r){
    while(l<r){
        [nums[r],nums[l]] = [nums[l],nums[r]];
        r--;
        l++;
    }
 return nums;
};

let nums = [1,2,3,4,5,6,7]
let k = 3;
 # output = [ 5, 6, 7, 1, 2, 3, 4]

console.log(rotate(nums,k))
