

//
 remove duplicate elements from the array and return the distinct elements using a Set(); 


function check(arr){
    
    const set = new Set();
    
    for(let i=0; i<arr.length; i++){
      
      set.add(arr[i])        
    
}

return set;
}

let arr = [1,2,3,4,5,6,3,2,1,5,4,6,9,8,7];

console.log(check(arr))
