#include<iostream>
using namespace std;

  // naive solution

    int RemoveDup(int arr[], int n){
    	int count = 0;
    	int res = 1;
    	for(int i=0; i<n; i++){ 
    
    		if(arr[i]!=arr[res]){	
//    			cout<<arr[i]<<endl;
    				count++;
				}
		  		
			}
			return count;
		}
		
	
	// Sec Approach
	
	int RemDup(int arr[],int n){
		int temp[n];
		 temp[0] = arr[0];
		 int res = 1;
		 
	for(int i=1; i<n; i++){
		if(temp[res-1] != arr[i]){
			temp[res] = arr[i];
			res++;
			
		}
	}
	  
	for(int i=0; i<res; i++){
		arr[i] = temp[i];
		return res;
	}	 
	}
	
	
	//  Approach 
	
	int RemDup2(int arr[], int n){
	     int res =1;
			for(int i=1; i<n; i++){
				if(arr[i] != arr[res-1]){
					arr[res] = arr[i];
					res++;
				}
				}
		return res;		
		
	}
	
  int main(){
  	int arr[] = {10,10,10};
//  	cout<<RemoveDup(arr,2)<<endl;
  	cout<<"Harmesh mahaur"<<endl; 
  	cout<<RemDup(arr,2);
  }
