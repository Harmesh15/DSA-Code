
function Binary_Search(nums, target) {
    
   let l = 0; 
   let h = nums.length;
   let mid=0;

   if(l==h){
    if(nums[l]==target){
        return l;
    }else{
        return -1;
    }
   }else{

   while(l<=h){
    mid = Math.floor((l+h)/2);
    
    if(nums[mid]==target){
        return mid;
    }
    if(nums[mid]>target){
        h = mid-1;
    }
    else{
        l = mid+1;
    }
   }
   return -1;
   }
};
